<view class="page-header">
	<text class="page-header-text">{{title}}</text>
</view>
<scroll-view class="page-body" scroll-y="true">
	<navigator url="../list/list?tb={{item.tb}}&brand={{item.brand}}" wx:for="{{classify}}">
		<view class="item" >
			<view class="meta">
				<text class="title">{{item.brand}}</text>
        <text class="sub-title"> 更新时间 ： {{item.gettime}} 分类总量 ：  {{item.count}}</text>
			</view>
		</view>
	</navigator>
</scroll-view>